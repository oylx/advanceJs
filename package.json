    "@types/chai": "^4.2.15",
    "@types/mocha": "^8.2.2",
    "babel-eslint": "^10.1.0",
    "chai": "^4.3.4",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
